{
  "name": "template",
  "version": "1.0.0",
  "description": "template",
  "main": "index.js",
  "config": {
    "path": "./public"
  },
  "scripts": {
    "lint": "eslint --fix ./src/js/*.js && eslint --fix ./src/js/app.js",
    "build:clean": "rimraf $npm_package_config_path",
    "build:webpack": "webpack --mode production",
    "build:moduledelete": "rimraf $npm_package_config_path/js/modules",
    "makefontdir": "mkdir -p $npm_package_config_path/font",
    "iconfont": "icon-font-generator ./src/img/svg/*.svg -o ./public/font/ --name icon --cssfontsurl ../font/ --csspath ./src/_stylus/_foundation/_iconFont.styl --html false --types 'ttf, woff, woff2, eot, svg' --json false --prefix c-icon-",
    "imagemin:build": "node imagemin.js",
    "imagemin:watch": "onchange './src/img' -- npm run imagemin:build",
    "dev": "webpack-dev-server --mode development --inline --content-base public/ --watch-content-base --open",
    "build": "run-s build:* makefontdir iconfont imagemin:build"
  },
  "keywords": [
    "template"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "globule": "^1.2.1",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "icon-font-generator": "^2.1.10",
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "jquery": "^3.4.1",
    "micromodal": "^0.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": [
    "last 2 version",
    "ie >= 11"
  ]
}
